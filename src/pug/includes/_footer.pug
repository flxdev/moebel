footer.footer#containerForm
    .container-footer
        .container-footer-contact-us
            .container-footer-contact-us-info.parallax(data-speed='22')
                h2.contact-us-info_title Cooperate <br> with us
                .contact-us-info_txt-q Here you can leave your request or to ask any question and we'll contact you.
                .contact-us-info_tel
                    a.link-tel(href='tel:+371 6 616 55 07') +371 6 616 55 07
                a(href='mailto:ops@himbalt.lv').contact-us-info_email ops@himbalt.lv
            .container-footer-contact-us-form.parallax(data-speed='8')
                form.js-form
                    .box-input
                        .form-label
                            label.input-label Full name*
                        .form-input
                            input(type='text').input-box.input-name
                    .box-input
                        .form-label
                            label.input-label Phone number*
                        .form-input
                            input(type='tel').input-box
                    .box-input
                        .form-label
                            label.input-label E-mail*
                        .form-input
                            input(type='email').input-box
                    .box-input
                        .form-label
                            label.input-label Message*
                        .form-input
                            textarea(type='text').input-box.textarea-box
                    .box-addfile-btn
                        .box-addfile-btn_container
                            .box-add-file
